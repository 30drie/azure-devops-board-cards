{
    "manifestVersion": 1,
    "id": "board-cards-dev",
    "publisher": "NickMyers",
    "version": "1.1.715",
    "name": "(DEV) Board cards for printing",
    "description": "Adds a 'Print' tab to Sprint boards, which displays all the work items in @currentIteration and triggers the browsers print dailog.",
    "public": false,
    "categories": ["Azure Boards"],
    "scopes": ["vso.work"],
    "repository": {
        "type": "git",
        "url": "https://github.com/nick-myers/azure-devops-board-cards"
    },
    "bugs": {
        "url": "https://github.com/nick-myers/azure-devops-board-cards/issues"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/board-cards.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "contributions": [
        {
            "id": "board-cards",
            "type": "ms.vss-web.tab",
            "description": "Print (DEV)",
            "targets": [
                "ms.vss-work-web.iteration-backlog-tabs"
            ],
            "properties": {
                "text": "Print (DEV)",
                "title": "Print (DEV)",
                "name": "Print (DEV)",
                "uri": "board-cards.html",
                "registeredObjectId": "board-cards"
            }
        },
        {
            "id": "configuration",
            "type": "ms.vss-web.control",
            "description": "The content to be displayed in the dialog",
            "targets": [],
            "properties": {
                "uri": "configuration.html"
            }
        }
    ],
    "files": [
        {
            "path": "board-cards.html",
            "addressable": true
        },
        {
            "path": "configuration.html",
            "addressable": true
        },
        {
            "path": "css/cards.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "css/print.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "css/screen.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "images",
            "addressable": true
        }
    ]
}