{
    "manifestVersion": 1,
    "id": "board-cards",
    "publisher": "NickMyers",
    "version": "3.4",
    "name": "Board cards for printing",
    "description": "Adds a 'Print' tab to Sprint boards, and allows users to select iterations, work item types and areas to print on A4 paper for putting on physical boards..",
    "public": true,
    "categories": ["Azure Boards"],
    "scopes": ["vso.work"],
    "repository": {
        "type": "git",
        "url": "https://github.com/nick-myers/azure-devops-board-cards"
    },
    "bugs": {
        "url": "https://github.com/nick-myers/azure-devops-board-cards/issues"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/board-cards.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "contributions": [
        {
            "id": "board-cards",
            "type": "ms.vss-web.tab",
            "description": "Print",
            "targets": [
                "ms.vss-work-web.iteration-backlog-tabs"
            ],
            "properties": {
                "text": "Print",
                "title": "Print",
                "name": "Print",
                "uri": "board-cards.html",
                "registeredObjectId": "board-cards"
            }
        }
    ],
    "files": [
        {
            "path": "board-cards.html",
            "addressable": true
        },
        {
            "path": "css/cards.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "css/screen.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "css/print.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "js/clamp.js",
            "contentType": "text/javascript",
            "addressable": true
        }
    ]
}