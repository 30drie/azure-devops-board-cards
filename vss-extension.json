{
    "manifestVersion": 1,
    "id": "board-cards",
    "publisher": "NickMyers",
    "version": "1.1.2",
    "name": "Board cards for printing",
    "description": "Adds a 'Print' tab to Sprint boards, which displays all the work items in @currentIteration and triggers the browsers print dailog.",
    "public": true,
    "categories": ["Azure Boards"],
    "scopes": ["vso.work"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/board-cards.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENCE"
        }
    },
    "contributions": [
        {
            "id": "board-cards",
            "type": "ms.vss-web.tab",
            "description": "Print",
            "targets": [
                "ms.vss-work-web.iteration-backlog-tabs"
            ],
            "properties": {
                "text": "Print",
                "title": "Print",
                "name": "Print",
                "uri": "board-cards.html",
                "registeredObjectId": "board-cards"
            }
        }
    ],
    "files": [
        {
            "path": "board-cards.html",
            "addressable": true
        },
        {
            "path": "css/print.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "css/cards.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "css/screen.css",
            "contentType": "text/css",
            "addressable": true
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "images",
            "addressable": true
        }
    ]
}